<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:windowvms="clr-namespace:Marketplace.WindowViewModels"
                    xmlns:windowviews="clr-namespace:Marketplace.WindowViews">

    <DataTemplate DataType="{x:Type windowvms:NavigationWindowVm}">
        <windowviews:NavigationWindowView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type windowvms:AuthWindowVm}">
        <windowviews:AuthWindowView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type windowvms:RegWindowVm}">
        <windowviews:RegWindowView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type windowvms:OrderWindowVm}">
        <windowviews:OrderWindowView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type windowvms:ProductWindowVm}">
        <windowviews:ProductWindowView/>
    </DataTemplate>

</ResourceDictionary>